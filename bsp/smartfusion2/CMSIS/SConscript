from building import *
import rtconfig

cwd = GetCurrentDir()
src	= Glob('*.c')

if rtconfig.PLATFORM == 'gcc':
    src += ['startup_gcc/startup_m2sxxx.S']
    
elif rtconfig.PLATFORM in ['armcc', 'armclang']:
    src += ['startup_arm/startup_m2sxxx.s']

CPPPATH = [cwd]

group = DefineGroup('CMSIS', src, depend = [''], CPPPATH = CPPPATH)

Return('group')
